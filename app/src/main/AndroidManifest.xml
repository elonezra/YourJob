<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ezra.elon.yourjob"

    >
    <uses-sdk android:minSdkVersion="4.0"
        android:targetSdkVersion="6.0"
         />

    <application
        android:allowBackup="true"

        android:icon="@drawable/myjoblogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FinishedState"
            android:label="@string/title_activity_finished_state"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ActivityForGame"></activity>
    </application>

</manifest>